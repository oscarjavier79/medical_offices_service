<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <custom-converters>
            <converter
                    type="com.mx.offices.customeConverters.MedicDTOToMedicConverter">
                <class-a>com.mx.offices.dto.MedicDto</class-a>
                <class-b>com.mx.offices.entity.Medic</class-b>
            </converter>
            <converter
                    type="com.mx.offices.customeConverters.MedicRemoveDTOToMedicConverter">
                <class-a>com.mx.offices.dto.MedicRemoveDto</class-a>
                <class-b>com.mx.offices.entity.Medic</class-b>
            </converter>
        </custom-converters>
    </configuration>

</mappings>